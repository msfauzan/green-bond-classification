{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "webapp/static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/",
      "dest": "webapp/static/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "webapp/static/$1"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9"
  }
}
